#include <stdio.h>

int n;
int k;
int A[100000];

int p(int m){
    return A[m] >= k;
}
int c(int n){
    return A[n-1] < k;
}

int main(){
  int i, lb, ub;
  printf("Please enter the number n:");
  scanf("%d", &n);
  printf("Please enter the number k:");
  scanf("%d", &k);
  for(i = 0; i < n; i++){
    scanf("%d", &A[i]);
  }
  lb = 0;
  ub = n-1;
  if (c(n)){
    ub = n;
  }
  while ( ub-lb > 1) {
    int m = (lb+ub) / 2;
    if (p(m)) {
        ub = m;
    }
    else {
        lb = m;
    }
  }
  printf("%d\n",ub);
  return 0;
}
